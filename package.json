{
  "name": "tomastrajan.com",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "npm run serve:ssr:dev",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "serve": "ng serve -o",
    "serve:ssr:dev": "ng run website:serve-ssr",
    "serve:ssr:prod": "npm run build:ssr && node dist/server/main.js",
    "build:ssr": "ng build --prod && ng run website:server:production --bundleDependencies all",
    "heroku-postbuild": "serve:ssr:prod",
    "format:write": "prettier projects/**/*.{ts,json,md,html} --write",
    "format:test": "prettier projects/**/*.{ts,json,md,html} --list-different",
    "prerender": "ng run website:prerender"
  },
  "engines": {
    "node": "10.x"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "@angular/animations": "^10.1.4",
    "@angular/cdk": "^10.2.3",
    "@angular/common": "^10.1.4",
    "@angular/compiler": "^10.1.4",
    "@angular/core": "^10.1.4",
    "@angular/forms": "^10.1.4",
    "@angular/material": "^10.2.3",
    "@angular/platform-browser": "^10.1.4",
    "@angular/platform-browser-dynamic": "^10.1.4",
    "@angular/platform-server": "^10.1.4",
    "@angular/router": "^10.1.4",
    "@nguniversal/express-engine": "^10.1.0",
    "@sendgrid/mail": "^7.2.6",
    "body-parser": "^1.19.0",
    "express": "^4.15.2",
    "express-useragent": "^1.0.15",
    "node-fetch": "^2.6.1",
    "rxjs": "~6.6.3",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1001.4",
    "@angular/cli": "~10.1.4",
    "@angular/compiler-cli": "~10.1.4",
    "@angular/language-service": "~10.1.4",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@nguniversal/builders": "^10.1.0",
    "@types/express": "^4.17.8",
    "@types/jasmine": "~3.5.14",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "dotenv": "^8.2.0",
    "husky": "^4.3.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.0.2",
    "protractor": "~7.0.0",
    "ts-loader": "^8.0.4",
    "ts-node": "~9.0.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.3",
    "webpack-cli": "^3.3.12"
  }
}
